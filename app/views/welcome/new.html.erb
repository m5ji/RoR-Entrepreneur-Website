<h1>Upload</h1>

<%= form_for :file, url: welcome_index_path  do |f| %>
  <p>
    <%= f.label :Name %><br>
    <%= f.text_field :Name %>
  </p>
 
  <p>
    <%= f.label :url %><br>
    <%= f.file_field :url %>
  </p>
 <p>
    <%= f.label :Description %><br>
    <%= f.text_area :Description %>
  </p>
  <p>
    <%= f.label :Price %><br>
    <%= f.text_field :Price %>
  </p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to "Back", welcome_index_path %>
